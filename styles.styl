*    
    box-sizing: border-box
    margin: 0
    padding: 0
body
    background-color: #EBEBE7
    font-family: 'Libre Baskerville', serif;
h1,h2,h3,h4
    line-height: 1.6
.container
    max-width: 1100px
    margin: auto
    overflow: hidden
    padding-left: 0 1rem
.logo > img
    display: block
    margin: auto
    width: 25%
.section-a
    background-image: url("images/imagex.jpg")
    background-position: right
    background-repeat: no-repeat    
    min-height: 200vh
.section-a .container   
    text-align: center 
    height: 525px   
a
    text-decoration: none
p
    line-height: 1.6
.buttons
    margin-top: 70px
.letter
    font-size: xxx-large
.letter_min
    font-size: x-large
.btn
    cursor: pointer
    border: 1px solid #f5a623
    font-size: xx-large;
    color: white
    background-color: #f5a623
    padding: 1rem 1rem 1rem 1rem 
    margin: 0px 25px
.section-b
.section-b .container
    margin: 6em 0
    text-align: center
.text_min
    margin-top: 45px
    text-align: justify
.logo-b > img
    width: 80%
.p_text
    margin: 0 120px
    font-size: x-large
.section-c
    text-align: center
.logo-c
    margin-top: 120px
.logo-c > img
    width: 80%
.p_text_only
    margin: 3rem   
.simbol-text    
    display: flex
    width: 354px
    margin-top: 15px
    text-align: left
.simbol-text > img
    width: 10%
.simbol-text > p
    margin-left: 10px
.container-simbols   
    display: flex
    flex-direction: column
    align-items: center
.grid
    display: grid
    margin 4em 0em -6em 0em
    grid-template-columns: repeat(4,200px)
    text-align: center
    justify-content: center
.grid img
    width: 200px
    height: 200px    
.wood_stick img
    width: 94%
.section-e
    min-height: 80vh
    text-align: center
.final-logo
    margin-top: 6em
.banner-img img
    margin-top: 10em
.final-p
    display: grid
    margin 4em 0em -6em 0em
    grid-template-columns: repeat(4,200px)
    text-align: center
    justify-content: center
.final-p > p
    color: #3d5d57;
    font-size: large
    font-weight: bold
.min-footer
    margin-top: 6em
.min-footer > p
    font-size: larger
@media(max-width:700px)
    .wood_stick img
        display: none
    .section-a .buttons
        display: grid
        grid-template-columns: 1fr
        text-align: center
    .section-a .buttons .btn
        margin: 0.5rem
        height: 67%
    .section-a .logo img
        width: 50%
    .section-a
        width: 100%
        height: 500px
        background-size: cover
        background-position: center top
    .section-b .p_text .text_min
        text-align: justify
    .section-b .buttons
        display: grid
        grid-template-columns: 1fr
        text-align: center
    .section-b .buttons .btn
        margin: 0.5rem
        height: 67%
    .section-c .p_text
        text-align: justify
    .section-d .grid        
        grid-template-columns: auto
        text-align: center
        margin: 0 auto
    .section-d .grid img
        width: 320px
        height: 320px
    .wood_stick img
        width: 55%
    .final-logo img
        width: 20rem
    .final-p
        display: grid
        grid-template-columns: 1fr
        text-align: center
    .banner-img img
        width: 100%
        text-align: center
    .min-footer > p
        text-align: center
        font-weight: small
        color: #3d5d57;
    .p_text
        text-align: justify
    


    

    